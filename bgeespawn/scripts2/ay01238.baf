  RandomNum(1,1)
  Global("AYSpawnRandomSetAR01238_0","%Undercity%",0)
  Global("AYSpawnRandomBeenSetAR01238","%Undercity%",0)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnRandomSetAR01238_0","%Undercity%",1)
    SetGlobal("AYSpawnRandomBeenSetAR01238","%Undercity%",1)
END

