IF
  Global("AYSpawnRandomBeenSetAR01238","%Undercity%",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR01238","%Undercity%",1)
    SetGlobal("AYSpawnResetAR01238","%Undercity%",1)
  RESPONSE #GOSPAWN
    Continue()
END

