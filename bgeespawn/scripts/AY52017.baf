IF
  Global("AYSpawnRandomBeenSetAR52017","%FirewineRuins%",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR52017","%FirewineRuins%",1)
    SetGlobal("AYSpawnResetAR52017","%FirewineRuins%",1)
  RESPONSE #GOSPAWN
    Continue()
END

