IF
  Global("AYSpawnRandomBeenSetAR52015","%FirewineRuins%",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR52015","%FirewineRuins%",1)
    SetGlobal("AYSpawnResetAR52015","%FirewineRuins%",1)
  RESPONSE #GOSPAWN
    Continue()
END

