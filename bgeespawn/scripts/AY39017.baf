  Range([PC],60)
  LevelPartyGT(1)
  LevelPartyLT(3)
  Global("AYSpawnResetAR39017","AR3901",0)
  Global("AYSpawnRandomSetAR39017_0","AR3901",1)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnResetAR39017","AR3901",1)
    CreateCreature("WOLFDR",[513.616],0)  // ~Dread Wolf~
    SetGlobalTimer("AYSpawnTimerAR39017","AR3901",AY_SPAWN_TIMER)
END

