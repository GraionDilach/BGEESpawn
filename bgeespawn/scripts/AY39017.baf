IF
  Global("AYSpawnRandomBeenSetAR39017","AR3901",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR39017","AR3901",1)
    SetGlobal("AYSpawnResetAR39017","AR3901",1)
  RESPONSE #GOSPAWN
    Continue()
END

IF
  RandomNum(1,1)
  Global("AYSpawnRandomSetAR39017_0","AR3901",0)
  Global("AYSpawnRandomBeenSetAR39017","AR3901",0)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnRandomSetAR39017_0","AR3901",1)
    SetGlobal("AYSpawnRandomBeenSetAR39017","AR3901",1)
END

