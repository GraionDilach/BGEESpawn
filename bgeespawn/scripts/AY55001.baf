    CreateCreature("GIBBER",[3958.783],0)  // ~Gibberling~
    CreateCreature("GIBBER",[3958.783],0)  // ~Gibberling~
    CreateCreature("GIBBER",[3958.783],0)  // ~Gibberling~
    SetGlobalTimer("AYSpawnTimerAR55001","%GibberlingMountains%",AY_SPAWN_TIMER)
