IF
  !Range([PC],75)

  !Range("GIBBER",75)
  Global("AYSpawnResetAR55001","%GibberlingMountains%",1)
  GlobalTimerExpired("AYSpawnTimerAR55001","%GibberlingMountains%")
THEN
  RESPONSE #100
    SetGlobal("AYSpawnResetAR55001","%GibberlingMountains%",0)
    SetGlobal("AYSpawnRandomBeenSetAR55001","%GibberlingMountains%",0)
    SetGlobal("AYSpawnRandomSetAR55001_0","%GibberlingMountains%",0)
END

