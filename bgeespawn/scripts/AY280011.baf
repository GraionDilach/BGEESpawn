IF
  Global("AYSpawnRandomBeenSetAR280011","%CoastWay%",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR280011","%CoastWay%",1)
    SetGlobal("AYSpawnResetAR280011","%CoastWay%",1)
  RESPONSE #GOSPAWN
    Continue()
END

